{
    "compilerOptions": {
        "baseUrl": ".",
        "paths": {
            "@assets/*": [
                "assets/*"
            ],
            "@data/*": [
                "src/data/*"
            ],
            "@components/*": [
                "src/components/*"
            ]
        },
        "types": [
            "vite-plugin-pwa/client"
        ],
        "target": "ES2023",
        "useDefineForClassFields": true,
        "lib": [
            "ES2023",
            "DOM",
            "DOM.Iterable"
        ],
        "module": "ESNext",
        "skipLibCheck": true,
        "moduleResolution": "bundler",
        "allowImportingTsExtensions": true,
        "resolveJsonModule": true,
        "isolatedModules": true,
        "noEmit": true,
        "jsx": "react-jsx",
        "strict": true,
        "noImplicitAny": false,
        "noUnusedLocals": true,
        "noUnusedParameters": false,
        "noFallthroughCasesInSwitch": true
    },
    "include": [
        "src",
        "./*.d.ts",
    ],
    "references": [
        {
            "path": "./tsconfig.node.json"
        }
    ]
}